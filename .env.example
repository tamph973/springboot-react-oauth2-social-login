# Default user credentials spring security
SPRING_SECURITY_USER_NAME=
SPRING_SECURITY_USER_PASSWORD=

# Frontend redirect after successful login (used in SecurityConfig)
FRONTEND_URL=

# GitHub OAuth2 registration (use real secrets in CI/CD or local .env.local)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Google OAuth2 registration (use real secrets in CI/CD or local .env.local)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=